{
  "name": "switcher-api",
  "version": "1.0.0",
  "description": "Feature Flag/Toogle API",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "env-cmd -e dev nodemon ./src/index.js",
    "prod": "env-cmd -e prod nodemon ./src/index.js",
    "test": "env-cmd -e test jest --runInBand",
    "test-admin": "env-cmd -e test jest -t 'ADMIN_SUITE'",
    "test-domain": "env-cmd -e test jest -t 'DOMAIN_SUITE'",
    "test-group": "env-cmd -e test jest -t 'GROUP_SUITE'",
    "test-config": "env-cmd -e test jest -t 'CONFIG_SUITE'",
    "test-strategy": "env-cmd -e test jest -t 'STRATEGY_SUITE'",
    "test-client": "env-cmd -e test jest -t 'CLIENT_SUITE'"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "feature",
    "flag"
  ],
  "author": "Rogerio Petruki",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcrypt": "^3.0.6",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "ip-cidr": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.9",
    "multer": "^1.4.2",
    "sharp": "^0.23.2",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2"
  }
}
